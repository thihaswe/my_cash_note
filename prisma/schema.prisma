// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int              @id @default(autoincrement())
  username         String @unique
  password        String
  gender  Gender
  dateOfBirth    String
}

model Note{
  id Int @id @default(autoincrement())
  categoryId  Int
  amount Int
}

model Category{
  id Int @id @default(autoincrement())
  assetUrl String
}

enum Gender{
  male
  female
}